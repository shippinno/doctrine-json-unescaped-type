{
    "name": "tanigami/doctrine-json-unescaped-type",
    "description": "Doctrine JSON type without escaping Unicode characters.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Hirofumi Tanigami",
            "email": "tanigami@gmail.com",
            "homepage": "https://tanigami.wtf",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">7.0",
        "ext-json": "*",
        "doctrine/dbal": "^2.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "mockery/mockery": "^1.2",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Tanigami\\DoctrineJsonUnescapedType\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tanigami\\DoctrineJsonUnescapedType\\": "tests/"
        }
    }
}
